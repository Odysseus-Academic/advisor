generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Group {
  id         Int      @id
  hash       String   @unique
  type       Boolean?
  coreqs     String
  course     Course?
  requires   Group[]  @relation("GroupReqs")
  requiredBy Group[]  @relation("GroupReqs")
}

model Course {
  id                 String       @id
  subject            String
  code               String
  level              Int
  title              String?
  repeatability      String?
  description        String?
  pathways           String?
  hours              String?
  groupId            Int?         @unique
  group              Group?       @relation(fields: [groupId], references: [id])
  sections           Section[]
  crosslist          Course[]     @relation("Crosslist")
  crosslistSymmetric Course[]     @relation("Crosslist")
  Instructor         Instructor[] @relation("InstructorCourses")
}

model Section {
  id             String     @id
  instructorName String
  course_id      String
  credits        String
  crn            Int
  enrollment     Int
  gpa            Float
  term           String
  title          String
  year           Int
  gradeData      Float[]
  course         Course     @relation(fields: [course_id], references: [id])
  instructor     Instructor @relation(fields: [instructorName], references: [id])
}

model Instructor {
  id             String    @id
  firstName      String
  lastName       String
  difficulty     Float
  rating         Float
  recommendedPct Float
  numRatings     Int
  sectionsTaught Int
  sections       Section[]
  courses        Course[]  @relation("InstructorCourses")
}
